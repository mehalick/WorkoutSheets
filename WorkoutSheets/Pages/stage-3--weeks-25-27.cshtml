@page

<style media="all">
	@@page {
		size: portrait;
	}

	#table th,
	#table td {
		padding: 10px 6px;
	}
</style>

@{
	var a = new Stage
	{
		Title = "Stage 3A — Weeks 25 - 27",
		SetsPerDay = [10, 10, 10],
		Exercises =
		[
			"Barbell Bench Press",
			"Seated Cable Row",
			"DB Incline Bench Press",
			"DB One-arm Row",
			"Bench Dip",
			"DB Seated Hammer Curl",
			"Knee-up Incline",
			"Curlup, Hands Shoulders",
			"Russian Twist",
			"Modified V-sit"
		],
		Supersets = [1, 3, 5, 7],
		HighlightColumns = true
	};

	var b = new Stage
	{
		Title = "Stage 3B — Weeks 25 - 27",
		SetsPerDay = [10, 10, 10],
		Exercises =
		[
			"Barbell Squat",
			"Deadlift",
			"Static Lunge",
			"Good Morning",
			"Calf Raise",
			"Barbell Shrug",
			"Toes to Sky",
			"Curlup, Legs in Air",
			"Side Raise Twist",
			"Pushup Hold"
		],
		Supersets = [1, 5, 7],
		HighlightColumns = true
	};

	var c = new Stage
	{
		Title = "Stage 3C — Weeks 25 - 27",
		SetsPerDay = [10, 10, 10],
		Exercises =
		[
			"Lat Pulldown",
			"Shoulder Press",
			"DB Lying Pullover",
			"DB Lat Raise",
			"EZ-Bar Curl",
			"Triceps Pushdown",
			"Knee Up, Incline",
			"Curlup, Arms Straight",
			"Russian Twist",
			"Modified V-sit"
		],
		Supersets = [1, 3, 5, 7],
		HighlightColumns = true
	};
}

<div class="row">
	<div class="col-12">
		<partial name="_StagePartial" model="a" />
	</div>
	<div class="col-12">
		<partial name="_StagePartial" model="b" />
	</div>
	<div class="col-12">
		<partial name="_StagePartial" model="c" />
	</div>
</div>
